select private.install_orm('{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "Project",
  "table": "prj",
  "comment": "Project Model",
  "privileges": {
    "all": {
      "create": "MaintainAllProjects",
      "read": "ViewAllProjects",
      "update": "MaintainAllProjects",
      "delete": "MaintainAllProjects"
    },
    "personal": {
      "create": "MaintainPersonalProjects",
      "read": "ViewPersonalProjects",
      "update": "MaintainPersonalProjects",
      "delete": "MaintainPersonalProjects",
      "properties": [
        "owner",
        "assignedTo"
       ]
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "prj_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "number",
      "attr": {
        "type": "String",
        "column": "prj_number",
        "isEditable": false
      }
    },
    {
      "name": "name",
      "attr": {
        "type": "String",
        "column": "prj_name"
      }
    },
    {
      "name": "notes",
      "attr": {
        "type": "String",
        "column": "prj_descrip"
      }
    },
    {
      "name": "startDate",
      "attr": {
        "type": "Date",
        "column": "prj_start_date"
      }
    },
    {
      "name": "dueDate",
      "attr": {
        "type": "Date",
        "column": "prj_due_date"
      }
    },
    {
      "name": "assignDate",
      "attr": {
        "type": "Date",
        "column": "prj_assigned_date"
      }
    },
    {
      "name": "completeDate",
      "attr": {
        "type": "Date",
        "column": "prj_completed_date"
      }
    },
    {
      "name": "assignedTo",
      "toOne": {
        "type": "XM.UserAccountInfo",
        "column": "prj_username",
        "inverse": "username"
      }
    },
    {
      "name": "projectStatus",
      "attr": {
        "type": "String",
        "column": "prj_status"
      }
    },
    {
      "name": "owner",
      "toOne": {
        "type": "XM.UserAccountInfo",
        "column": "prj_owner_username",
        "inverse": "username"
      }
    },
    {
      "name": "comments",
      "toMany": {
        "type": "XM.ProjectComment",
        "column": "prj_id",
        "inverse": "project"
      }
    },
    {
      "name": "tasks",
      "toMany": {
        "type": "XM.ProjectTask",
        "column": "prj_id",
        "inverse": "project"
      }
    },
    {
      "name": "contacts",
      "toMany": {
        "type": "XM.ProjectContact",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "items",
      "toMany": {
        "type": "XM.ProjectItem",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "files",
      "toMany": {
        "type": "XM.ProjectFile",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "images",
      "toMany": {
        "type": "XM.ProjectImage",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "urls",
      "toMany": {
        "type": "XM.ProjectUrl",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "projects",
      "toMany": {
        "type": "XM.ProjectProject",
        "column": "prj_id",
        "inverse": "source"
      }
    }
  ],
  "extensions": [
    {
      "table": "recur",
      "properties": [
        {
          "name": "fkey1",
          "relation": {
            "type": "Number",
            "column": "recur_parent_id",
            "inverse": "prj_id"
          }
        },
        {
          "name": "fkey2",
          "relation": {
            "type": "Number",
            "column": "recur_parent_type",
            "value": "J"
          }
        },
        {
          "name": "recurrencePeriod",
          "attr": {
            "type": "Date",
            "column": "recur_period"
          }
        },
        {
          "name": "recurrenceFrequency",
          "attr": {
            "type": "Date",
            "column": "recur_freq"
          }
        },
        {
          "name": "recurrenceStart",
          "attr": {
            "type": "Date",
            "column": "recur_start"
          }
        },
        {
          "name": "recurrenceEnd",
          "attr": {
            "type": "Date",
            "column": "recur_end"
          }
        },
        {
          "name": "recurrenceMax",
          "attr": {
            "type": "Date",
            "column": "recur_max"
          }
        }
      ]
    }
  ],
  "isSystem": true
}')