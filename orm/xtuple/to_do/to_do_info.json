{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "ToDoInfo",
  "table": "todoitem",
  "comment": "Todo Info Map",
  "privileges": {
    "all": {
      "create": false,
      "read": true,
      "update": false,
      "delete": false
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "todoitem_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "name",
      "attr": {
        "type": "String",
        "column": "todoitem_name",
        "isRequired": true
      }
    },
    {
      "name": "description",
      "attr": {
        "type": "String",
        "column": "todoitem_description"
      }
    },
    {
      "name": "contact",
      "toOne": {
        "isNested": true,
        "type": "ContactInfo",
        "column": "todoitem_cntct_id"
      }
    },
    {
      "name": "status",
      "attr": {
        "type": "String",
        "column": "todoitem_status",
        "defaultValue": "N"
      }
    },
    {
      "name": "isActive",
      "attr": {
        "type": "Boolean",
        "column": "todoitem_active",
        "defaultValue": true
      }
    },
    {
      "name": "dueDate",
      "attr": {
        "type": "Date",
        "column": "todoitem_due_date",
        "isRequired": true
      }
    },
    {
      "name": "priority",
      "toOne": {
        "type": "Priority",
        "column": "todoitem_priority_id"
      }
    },
    {
      "name": "assignedTo",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "todoitem_username",
        "inverse": "username"
      }
    }
  ],
  "isSystem": true
}
