{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "PayableApplication",
  "table": "xt.apapply",
  "comment": "Payable Application Map",
  "privileges": {
    "all": {
      "create": false,
      "read": "ViewAPOpenItems",
      "update": false,
      "delete": false
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "apapply_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "vendor",
      "toOne": {
        "type": "Vendor",
        "column": "apapply_vend_id",
        "inverse" : "guid"
      }
    },
    {
      "name": "payable",
      "toOne": {
        "type": "Payable",
        "column": "apopen_id",
        "inverse" : "guid"
      }
    },
    {
      "name": "documentType",
      "attr": {
        "type": "String",
        "column": "doctype"
      }
    },
    {
      "name": "documentNumber",
      "attr": {
        "type": "String",
        "column": "apapply_source_docnumber"
      }
    },
    {
      "name": "postDate",
      "attr": {
        "type": "Date",
        "column": "apapply_postdate"
      }
    },
    {
      "name": "amount",
      "attr": {
        "type": "Number",
        "column": "apapply_amount"
      }
    },
    {
      "name": "currency",
      "toOne": {
        "type": "Currency",
        "column": "apapply_curr_id"
      }
    },
    {
      "name": "paid",
      "attr": {
        "type": "Number",
        "column": "apapply_target_paid"
      }
    },    
    {
      "name": "createdBy",
      "attr": {
        "type": "String",
        "column": "apapply_username"
      }
    }
  ],
  "isSystem": true
}
