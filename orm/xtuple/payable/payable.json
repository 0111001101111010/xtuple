{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "Payable",
  "table": "apopen",
  "idSequenceName": "apopen_apopen_id_seq",
  "orderSequence": "APMemoNumber",
  "comment": "Receivable Map",
  "privileges": {
    "all": {
      "create": "MaintainAPMemos",
      "read": "ViewAPMemos",
      "update": "MaintainAPMemos",
      "delete": "MaintainAPMemos"
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "apopen_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "vendor",
      "toOne": {
        "isNested" : true,
        "type": "VendorInfo",
        "column": "apopen_vend_id",
        "inverse" : "guid",
        "isRequired" : true,
        "isEditable": false
      }
    },
    {
      "name": "documentDate",
      "attr": {
        "type": "Date",
        "column": "apopen_docdate",
        "isRequired": true,
        "isEditable": false
      }
    },
    {
      "name": "dueDate",
      "attr": {
        "type": "Date",
        "column": "apopen_duedate",
        "isRequired": true
      }
    },
    {
      "name": "documentType",
      "attr": {
        "type": "String",
        "column": "apopen_doctype",
        "isRequired": true,
        "isEditable": false
      }
    },
    {
      "name": "number",
      "attr": {
        "type": "String",
        "column": "apopen_docnumber",
        "isRequired": true,
        "isEditable": false
      }
    },
    {
      "name": "orderNumber",
      "attr": {
        "type": "String",
        "column": "apopen_ponumber",
        "isEditable": false
      }
    },
    {
      "name": "terms",
      "toOne": {
        "type": "Terms",
        "column": "apopen_terms_id",
        "isEditable": false,
        "isRequired": true,
        "defaultValue": -1
      }
    },
    {
      "name": "payableStatus",
      "toOne": {
        "type": "String",
        "column": "apopen_status",
        "defaultValue": "O",
        "isRequired": true,
        "isEditable": false
      }
    },
    {
      "name": "amount",
      "attr": {
        "type": "Money",
        "column": "apopen_amount",
        "isRequired": true
      }
    },
    {
      "name": "currency",
      "toOne": {
        "type": "Currency",
        "column": "apopen_curr_id",
        "isRequired": true,
        "isEditable": false,
        "defaultValue": "baseCurrency"
      }
    },
    {
      "name": "currencyRate",
      "attr": {
        "type": "Number",
        "column": "apopen_curr_rate",
        "isEditable": false
      }
    },
    {
      "name": "adjustmentTaxes",
      "toMany": {
        "isNested": true,
        "type": "PayableTaxAdjustment",
        "column": "apopen_id",
        "inverse": "payable"
      }
    },
    {
      "name": "notes",
      "attr": {
        "type": "String",
        "column": "apopen_notes"
      }
    },
    {
      "name": "applications",
      "toMany": {
        "isNested": true,
        "type": "PayableApplication",
        "column": "apopen_id",
        "inverse": "payable"
      }
    },   
    {
      "name": "isOpen",
      "attr": {
        "type": "Date",
        "column": "apopen_open",
        "defaultValue": true,
        "isEditable": false
      }
    },
    {
      "name": "closeDate",
      "attr": {
        "type": "Date",
        "column": "apopen_closedate",
        "isEditable": false
      }
    },
    {
      "name": "createdBy",
      "attr": {
        "type": "String",
        "column": "apopen_username",
        "isEditable": false,
        "defaultValue": "currentUser"
      }
    }
  ],
  "isSystem": true
}
