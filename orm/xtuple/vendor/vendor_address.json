{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "VendorAddress",
  "table": "vendaddrinfo",
  "comment": "Vendor Address Map",
  "privileges": {
    "all": {
      "create": "MaintainVendorAddresses",
      "read": "ViewVendorAddresses",
      "update": "MaintainVendorAddresses",
      "delete": "MaintainVendorAddresses"
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "vendaddr_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "vendor",
      "toOne": {
        "type": "Vendor",
        "column": "vendaddr_vend_id"
      }
    },
    {
      "name": "code",
      "attr": {
        "type": "String",
        "column": "vendaddr_code",
        "isEditable": false
      }
    },
    {
      "name": "name",
      "attr": {
        "type": "String",
        "column": "vendaddr_name",
        "isEditable": false
      }
    },
    {
      "name": "notes",
      "attr": {
        "type": "String",
        "column": "vendaddr_comments"
      }
    },
    {
      "name": "contact",
      "toOne": {
        "isNested": true,
        "type": "ContactInfo",
        "column": "vendaddr_cntct_id"
      }
    },
    {
      "name": "address",
      "toOne": {
        "isNested": true,
        "type": "AddressInfo",
        "column": "vendaddr_addr_id"
      }
    }
  ],
  "isNestedOnly": true,
  "isSystem": true
}


