{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "IncidentInfo",
  "table": "incdt",
  "comment": "Incident Info Map",
  "privileges": {
    "all": {
      "create": false,
      "read": "ViewAllIncidents",
      "update": false,
      "delete": false
    },
    "personal": {
      "create": false,
      "read": true,
      "update": false,
      "delete": false,
      "properties": [
        "owner",
        "assignedTo"
       ]
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "incdt_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "number",
      "attr": {
        "type": "Number",
        "column": "incdt_number",
        "isEditable": false
      }
    },
    {
      "name": "description",
      "attr": {
        "type": "String",
        "column": "incdt_summary",
        "isEditable": false
      }
    },
    {
      "name": "account",
      "toOne": { 
        "type": "AccountInfo",
        "column": "incdt_crmacct_id"
      }
    },
    {
      "name": "owner",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "incdt_owner_username",
        "inverse": "username"
      }
    },
    {
      "name": "assignedTo",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "incdt_assigned_username",
        "inverse": "username"
      }
    }
  ],
  "isSystem": true
}
