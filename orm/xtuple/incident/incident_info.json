{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "IncidentInfo",
  "table": "incdt",
  "comment": "Incident Info Map",
  "privileges": {
    "all": {
      "create": false,
      "read": "ViewAllIncidents",
      "update": false,
      "delete": false
    },
    "personal": {
      "create": false,
      "read": true,
      "update": false,
      "delete": false,
      "properties": [
        "owner",
        "assignedTo"
       ]
    }
  },
  "properties": [
    {
      "name": "id",
      "attr": {
        "type": "Number",
        "column": "incdt_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "number",
      "attr": {
        "type": "String",
        "column": "incdt_number",
        "isEditable": false
      }
    },
    {
      "name": "description",
      "attr": {
        "type": "String",
        "column": "incdt_summary",
        "isEditable": false
      }
    },
    {
      "name": "status",
      "attr": {
        "type": "String",
        "column": "incdt_status"
      }
    },
    {
      "name": "category",
      "toOne": {
        "type": "IncidentCategory",
        "column": "incdt_incdtcat_id",
        "isRequired": true
      }
    },
    {
      "name": "account",
      "toOne": { 
        "isNested": true,
        "type": "AccountInfo",
        "column": "incdt_crmacct_id"
      }
    },
    {
      "name": "contact",
      "toOne": {
        "isNested": true,
        "type": "ContactInfo",
        "column": "incdt_cntct_id",
        "isRequired": true
      }
    },
    {
      "name": "priority",
      "toOne": {
        "type": "Priority",
        "column": "incdt_incdtpriority_id"
      }
    },
    {
      "name": "owner",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "incdt_owner_username",
        "inverse": "username"
      }
    },
    {
      "name": "assignedTo",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "incdt_assigned_username",
        "inverse": "username"
      }
    },
    {
      "name": "updated",
      "attr": {
        "type": "Date",
        "column": "incdt_updated"
      }
    }
  ],
  "isSystem": true
}
