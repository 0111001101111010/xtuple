          "table": "comment",
          "relations": [
            {
              "column": "comment_source_id",
              "inverse": "guid"
            },
            {
              "column": "comment_source",
              "value": "T"
            }
          ]
        }
      }
    },
    {
      "name": "characteristics",
      "toMany": {
        "type": "ContactCharacteristic",
        "column": "cntct_id",
