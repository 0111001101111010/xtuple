    {
      "name": "email",
      "toMany": {
        "type": "ContactEmail",
        "column": "cntct_id",
        "inverse": "contact",
        "isMaster": true
      }
    },
    {
      "name": "comments",
      "toMany": {
        "type": "ContactComment",
