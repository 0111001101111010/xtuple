{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "ProjectInfo",
  "table": "prj",
  "comment": "Project Info Map",
  "privileges": {
    "all": {
      "create": false,
      "read": "ViewAllProjects",
      "update": false,
      "delete": false
    },
    "personal": {
      "create": false,
      "read": true,
      "update": false,
      "delete": false,
      "properties": [
        "owner",
        "assignedTo"
       ]
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "prj_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "number",
      "attr": {
        "type": "String",
        "column": "prj_number"
      }
    },
    {
      "name": "name",
      "attr": {
        "type": "String",
        "column": "prj_name"
      }
    },
    {
      "name": "status",
      "attr": {
        "type": "String",
        "column": "prj_status"
      }
    },
    {
      "name": "dueDate",
      "attr": {
        "type": "Date",
        "column": "prj_due_date"
      }
    },   
    {
      "name": "assignedTo",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "prj_username",
        "inverse": "username"
      }
    },
    {
      "name": "owner",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "prj_owner_username",
        "inverse": "username"
      }
    },
    {
      "name": "account",
      "toOne": {
        "isNested": true,
        "type": "AccountInfo",
        "column": "prj_crmacct_id"
      }
    },
    {
      "name": "tasks",
      "toMany": {
        "isNested": true,
        "type": "ProjectTask",
        "column": "prj_id",
        "inverse": "project"
      }
    }
  ],
  "isSystem": true
}
