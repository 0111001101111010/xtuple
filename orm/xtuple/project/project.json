{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "Project",
  "table": "prj",
  "idSequenceName": "public.prj_prj_id_seq",
  "comment": "Project Map",
  "privileges": {
    "all": {
      "create": "MaintainAllProjects",
      "read": "ViewAllProjects",
      "update": "MaintainAllProjects",
      "delete": "MaintainAllProjects"
    },
    "personal": {
      "create": "MaintainPersonalProjects",
      "read": "ViewPersonalProjects",
      "update": "MaintainPersonalProjects",
      "delete": "MaintainPersonalProjects",
      "properties": [
        "owner",
        "assignedTo"
       ]
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "prj_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "number",
      "attr": {
        "type": "String",
        "column": "prj_number",
        "isEditable": false,
        "isRequired": true
      }
    },
    {
      "name": "name",
      "attr": {
        "type": "String",
        "column": "prj_name",
        "isRequired": true
      }
    },
    {
      "name": "notes",
      "attr": {
        "type": "String",
        "column": "prj_descrip"
      }
    },
    {
      "name": "startDate",
      "attr": {
        "type": "Date",
        "column": "prj_start_date"
      }
    },
    {
      "name": "dueDate",
      "attr": {
        "type": "Date",
        "column": "prj_due_date",
        "isRequired": true
      }
    },
    {
      "name": "assignDate",
      "attr": {
        "type": "Date",
        "column": "prj_assigned_date"
      }
    },
    {
      "name": "completeDate",
      "attr": {
        "type": "Date",
        "column": "prj_completed_date"
      }
    },
    {
      "name": "assignedTo",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "prj_username",
        "inverse": "username",
        "defaultValue": "currentUser"
      }
    },
    {
      "name": "projectStatus",
      "attr": {
        "type": "String",
        "column": "prj_status",
        "isRequired": true,
        "defaultValue": "P"
      }
    },
    {
      "name": "owner",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "prj_owner_username",
        "inverse": "username",
        "defaultValue": "currentUser"
      }
    },
    {
      "name": "recurrences",
      "toMany": {
        "type": "ProjectRecurrence",
        "column": "prj_id",
        "inverse": "project"
      }
    },
    {
      "name": "comments",
      "toMany": {
        "isNested": true,
        "type": "ProjectComment",
        "column": "prj_id",
        "inverse": "project",
        "deleteDelegate": {
          "table": "comment",
          "relations": [
            {
              "column": "comment_source_id",
              "inverse": "guid"
            },
            {
              "column": "comment_source",
              "value": "J"
            }
          ]
        }
      }
    },
    {
      "name": "tasks",
      "toMany": {
        "isNested": true,
        "type": "ProjectTask",
        "column": "prj_id",
        "inverse": "project"
      }
    },
    {
      "name": "accounts",
      "toMany": {
        "isNested": true,
        "type": "ProjectAccount",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "contacts",
      "toMany": {
        "isNested": true,
        "type": "ProjectContact",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "items",
      "toMany": {
        "isNested": true,
        "type": "ProjectItem",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "files",
      "toMany": {
        "isNested": true,
        "type": "ProjectFile",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "images",
      "toMany": {
        "isNested": true,
        "type": "ProjectImage",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "urls",
      "toMany": {
        "isNested": true,
        "type": "ProjectUrl",
        "column": "prj_id",
        "inverse": "source"
      }
    },
    {
      "name": "projects",
      "toMany": {
        "isNested": true,
        "type": "ProjectProject",
        "column": "prj_id",
        "inverse": "source"
      }
    }
  ],
  "isSystem": true,
  "sourceCode": "J"
}
