{
  "context": "xtuple",
  "nameSpace": "XM",
  "type": "ContactInfo",
  "table": "cntct",
  "comment": "Contact Info Map",
  "privileges": {
    "all": {
      "create": false,
      "read": "ViewAllContacts",
      "update": false,
      "delete": false
    },
    "personal": {
      "create": false,
      "read": true,
      "update": false,
      "delete": false,
      "properties": [
        "owner"
       ]
    }
  },
  "properties": [
    {
      "name": "guid",
      "attr": {
        "type": "Number",
        "column": "cntct_id",
        "isPrimaryKey": true
      }
    },
    {
      "name": "name",
      "attr": {
        "type": "String",
        "column": "cntct_name",
        "isEditable": false
      }
    },
    {
      "name": "firstName",
      "attr": {
        "type": "String",
        "column": "cntct_first_name"
      }
    },
    {
      "name": "lastName",
      "attr": {
        "type": "String",
        "column": "cntct_last_name"
      }
    },
    {
      "name": "jobTitle",
      "attr": {
        "type": "String",
        "column": "cntct_title",
        "isEditable": false
      }
    },
    {
      "name": "phone",
      "attr": {
        "type": "String",
        "column": "cntct_phone",
        "isEditable": false
      }
    },
    {
      "name": "alternate",
      "attr": {
        "type": "String",
        "column": "cntct_phone2",
        "isEditable": false
      }
    },
    {
      "name": "fax",
      "attr": {
        "type": "String",
        "column": "cntct_fax",
        "isEditable": false
      }
    },
    {
      "name": "primaryEmail",
      "attr": {
        "type": "String",
        "column": "cntct_email",
        "isEditable": false
      }
    },
    {
      "name": "webAddress",
      "attr": {
        "type": "String",
        "column": "cntct_webaddr",
        "isEditable": false
      }
    },
    {
      "name": "address",
      "toOne": {
        "isNested": true,
        "type": "AddressInfo",
        "column": "cntct_addr_id"
      }
    },
    {
      "name": "owner",
      "toOne": {
        "isNested": true,
        "type": "UserAccountInfo",
        "column": "cntct_owner_username",
        "inverse": "username"
      }
    },
    {
      "name": "account",
      "toOne": {
        "isNested": true,
        "type": "AccountInfo",
        "column": "cntct_crmacct_id"
      }
    }
  ],
  "isSystem": true
}
